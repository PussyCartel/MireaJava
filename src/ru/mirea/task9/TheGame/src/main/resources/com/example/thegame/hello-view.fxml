<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.thegame.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="text1" text="Result: unknown"/>
    <Label fx:id="text2" text="Winner: unknown"/>
    <Label fx:id="text3" text="Last Scorer: N/A"/>
    <Button text="AC Milan" onAction="#onClick1"/>
    <Button text = "Real Madrid" onAction = "#onClick2"/>
</VBox>
